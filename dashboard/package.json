{
	"name": "dashboard",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prisma:generate": "echo 'Running generate in: ' && pwd && npx prisma generate",
		"prisma:move": "echo 'Copying prisme files in: ' && pwd && ls -al && cp ./prisma/schema.prisma ./build/server/sk_render/ && cp ./node_modules/.prisma/client/*.dylib.node ./build/server/sk_render"
	},
	"dependencies": {
		"@prisma/client": "4.16.1"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@fontsource/merriweather": "^5.0.3",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"@types/luxon": "^3.3.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"chart.js": "^4.3.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.26.0",
		"luxon": "^3.3.0",
		"mongodb": "^5.6.0",
		"prettier": "^2.8.0",
		"prisma": "^4.16.1",
		"prettier-plugin-svelte": "^2.8.1",
		"set-cookie-parser": "^2.6.0",
		"svelte": "^4.0.0",
		"svelte-adapter-azure-swa": "^0.16.0",
		"svelte-check": "^3.0.1",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.3.0",
		"vitest": "^0.25.3"
	},
	"type": "module"
}
